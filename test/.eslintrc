{
    "globals": {
        "describe": false,
        "it": false,
        "specify": false,
        "before": false,
        "after": false
    },
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "curly": 1
    },
    "env": {
        "node": true
    }
}
